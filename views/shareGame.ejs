
<div id="summonerInfo" 
     data-region="<%= region %>" 
     data-summoner-name="<%= summonerName %>" 
     data-tag="<%= tag %>" 
     style="display: none;">
</div>
<div class="inner">
  <div class="banner-sides">
    <a href="#"><div class="banner-ad-sides banner-ad-left"></div></a>
  </div>
  <div class="board">
